<div class="container" style="margin-top: 50px;">
    <div class="card"> 
    	<div class="card-header">
			Add Outing Details
		</div>
        <div class="card-body">
            <%- include("./partials/messages.ejs") %> 
            <form action="/outing/add/1" method="post">
                <div class="form-group">
                    <label>In_Time:</label>
                    <input type="text" class="form-control" name="In_Time" value="<%= In_Time %>" autocomplete="off">
                </div>
                <div class="form-group">
                    <label>Out_Time:</label>
                    <input type="text" class="form-control" name="Out_Time" value="<%= Out_Time %>" autocomplete="off">
                </div>
                <div class="form-group">
                    <label>Out_Date:</label>
                    <input type="text" class="form-control" name="Out_Date" value="<%= Out_Date %>" autocomplete="off">
                </div>
                <div class="form-group">
                    <label>In_Date:</label>
                    <input type="text" class="form-control" name="In_Date" value="<%= In_Date %>" autocomplete="off">
                </div>
                <div class="form-group">
                    <label>Student_ID:</label>
                    <input type="text" class="form-control" name="Student_ID" value="<%= Student_ID %>" autocomplete="off">
                </div>
                <div class="form-group">
                    <input type="submit" class="btn btn-info" value="Add"/>
                </div>
            </form>
        </div>
    </div>
</div>