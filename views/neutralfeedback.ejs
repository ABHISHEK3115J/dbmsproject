<div class="card text-center">
    <div class="card-header">
      <ul class="nav nav-tabs card-header-tabs">
        <li class="nav-item">
          <a class="nav-link" href="/feedback/<%= ide %> ">Positive</a>
        </li>
        <li class="nav-item">
          <a class="nav-link " href="/feedback/Negative/<%= ide %> ">Negative</a>
        </li>
        <li class="nav-item">
          <a class="nav-link active" href="/feedback/Neutral/<%= ide  %> ">Neutral</a>
        </li>
      </ul>
    </div>
    <div class="row">
      <%if(data.length){%>
      <% for(var i = 0; i< data.length; i++) { %>
      <div class="col-sm-4">
        <div class="card">
          <div class="card-body">
            <p class="card-text">ID:<%=data[i].rvce_ID%></p>
            <p class="card-text">Hostel_ID:<%=data[i].Hostel_ID%></p>
            <p class="card-text">feedback:<%=data[i].Text%></p>
            <a href="/feedback/edit/<%= data[i]._id %> " class="btn btn-primary">Edit</a>
            <a href="/feedback/delete/<%= data[i].Hostel_ID %>/<%= data[i]._id %>  " class="btn btn-danger">Delete</a>
          </div>
        </div>
      </div>
      <% } %> 
      <% } else { %>
       <p style="position: relative;text-justify:auto;">No feedbacks available!!</p>
      <% }  %>  
      </div>
  </div>
  
  
  
  